    "@amarty/models": {
      "projectType": "library",
      "root": "projects/amarty/models",
      "sourceRoot": "projects/amarty/models/lib",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/amarty/models/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/amarty/models/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/amarty/models/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/amarty/models/tsconfig.spec.json",
            "polyfills": ["zone.js", "zone.js/testing"]
          }
        }
      }
    },
