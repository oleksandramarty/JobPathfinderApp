{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "@amarty/shared": {
      "projectType": "library",
      "root": "projects/amarty/shared",
      "sourceRoot": "projects/amarty/shared/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/amarty/shared/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/amarty/shared/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/amarty/shared/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/amarty/shared/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
